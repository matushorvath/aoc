{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "cwd": "${fileDirname}"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "src/${fileBasename}"
            ],
            "cwd": "${fileDirname}/..",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch benchmark a.ts",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "args": [
                "src/a.ts"
            ],
            "cwd": "${workspaceFolder}/2019/benchmark",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "sourceMaps": true
        }
    ]
}